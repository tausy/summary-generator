<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="421.0" prefWidth="400.0"
	xmlns="http://javafx.com/javafx/11.0.1"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="controllers.GuestController" >
	<children>
		<AnchorPane maxHeight="-1.0"
			maxWidth="400.0" minWidth="400.0" prefHeight="500.0"
			prefWidth="400.0" style="-fx-background-color: #2D3447;"
			VBox.vgrow="ALWAYS">
			<children>
				<Label layoutX="83.0" layoutY="25.0"
					prefHeight="29.0" prefWidth="181.0" text="Road Accidents"
					textFill="#2196f3">
					<font>
						<Font size="24.0" />
					</font>
				</Label>
				<Label layoutX="264.0" layoutY="25.0"
					prefHeight="17.0" prefWidth="74.0" text="Leeds" textFill="#a0a2ab">
					<font>
						<Font name="System Bold" size="24.0" />
					</font>
				</Label>
				<Button fx:id="btnLogin" layoutX="127.0" layoutY="332.0"
					mnemonicParsing="false" prefHeight="32.0" prefWidth="150.0"
					style="-fx-background-color: #2196f3;" text="Login"
					textFill="WHITE" onAction="#login" >
					<font>
						<Font size="14.0" />
					</font>
				</Button>
				<TextField fx:id="user" layoutX="117.0" layoutY="107.0"
					promptText="Username" />
				<TextField fx:id="firstname" layoutX="117.0" layoutY="147.0"
					promptText="First name" />
				<ComboBox fx:id="gender" layoutX="117.0" layoutY="267.0"
					promptText="Gender" />
				<TextField fx:id="guestno" layoutX="118.0" layoutY="187.0"
					promptText="Guest No" />
				<DatePicker fx:id="dob" layoutX="117.0" layoutY="227.0"
					prefHeight="27.0" prefWidth="167.0" />
			</children>
		</AnchorPane>
	</children>
</VBox>
