<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="325.0" prefWidth="500.0"
	xmlns="http://javafx.com/javafx/11.0.1"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="controllers.HomepageController">
	<children>
		<AnchorPane maxHeight="-1.0" maxWidth="-1.0"
			prefHeight="367.0" prefWidth="640.0"
			style="-fx-background-color: #2D3447;" VBox.vgrow="ALWAYS">
			<children>
				<Label layoutX="127.0" layoutY="54.0"
					prefHeight="29.0" prefWidth="181.0" text="Road Accidents"
					textFill="#2196f3">
					<font>
						<Font size="24.0" />
					</font>
				</Label>
				<Label layoutX="308.0" layoutY="54.0"
					prefHeight="17.0" prefWidth="74.0" text="Leeds" textFill="#a0a2ab">
					<font>
						<Font name="System Bold" size="24.0" />
					</font>
				</Label>
				<Button fx:id="btnExit" layoutX="274.0" layoutY="201.0"
					mnemonicParsing="false" prefHeight="32.0" prefWidth="108.0"
					style="-fx-background-color:#2196f3;" text="Exit"
					textFill="WHITE" onAction="#exit">
					<font>
						<Font size="14.0" />
					</font>
				</Button>
				<Label layoutX="124.0" layoutY="280.0" prefHeight="18.0"
					prefWidth="261.0"
					text="* None of the user information is stored by our system"
					textFill="#a2a0ab">
					<font>
						<Font size="10.0" />
					</font>
				</Label>
				<Button fx:id="btnSignup" layoutX="127.0" layoutY="201.0"
					mnemonicParsing="false" prefHeight="32.0" prefWidth="108.0"
					style="-fx-background-color:#2196f3;" text="Sign Up"
					textFill="WHITE" onAction="#signup">
					<font>
						<Font size="14.0" />
					</font>
				</Button>
				<Button fx:id="btnLogin" layoutX="127.0" layoutY="143.0"
					mnemonicParsing="false" prefHeight="32.0" prefWidth="254.0"
					style="-fx-background-color:#2196f3;" text="Login &gt;&gt;"
					textFill="WHITE" onAction="#login">
					<font>
						<Font size="14.0" />
					</font>
				</Button>
			</children>
		</AnchorPane>
	</children>
</VBox>
